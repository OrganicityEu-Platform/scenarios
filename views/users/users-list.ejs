<% include ../header %>

    <div class="row">
        <h1>Users</h1>

		#Users: <%=users.length %>

		<ul>
		    <% users.forEach(function(u){ %>
				<li>
					<button data-id="<%=u.uuid %>" class="userEdit   btn btn-default">EDIT</button>
					<button data-id="<%=u.uuid %>" class="userDelete btn btn-default btn-danger">DELETE</button>
					<a href="/users/<%=u.uuid %>">User <%=u.uuid %></a>
				</li>
		    <% }) %>
		</ul>

    </div>

<% include ../footer %>
